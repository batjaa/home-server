# vim: set ft=yaml:
global:
    scrape_interval: 5s
    scrape_timeout: 5s

scrape_configs:
    - job_name: services
      metrics_path: /metrics
      static_configs:
          - targets:
                - 'localhost:9090'
                - '{{ lan_base }}.5:9100'
                - '{{ lan_base }}.5:9256'
